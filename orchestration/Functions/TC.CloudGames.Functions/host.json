{
  "version": "2.0",
  "extensions": {
    "serviceBus": {
      "prefetchCount": 0,
      "autoCompleteMessages": true,
      "maxConcurrentCalls": 8,
      "maxAutoLockRenewalDuration": "00:05:00",
      "maxConcurrentSessions": 8,
      "clientRetryOptions": {
        "mode": "exponential",
        "tryTimeout": "00:01:00",
        "delay": "00:00:00.80",
        "maxDelay": "00:01:00",
        "maxRetries": 3
      }
    }
  },
  "logging": {
    "logLevel": {
      "default": "Information",
      "Microsoft.Azure.WebJobs.Extensions.ServiceBus": "Debug"
    }
  }
}

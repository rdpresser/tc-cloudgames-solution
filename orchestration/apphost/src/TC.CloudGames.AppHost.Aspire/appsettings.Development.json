{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Aspire.Hosting.Dcp": "Warning"
    }
  },
  "MessageBroker": {
    "Type": "RabbitMQ"
  },
  "Database": {
    "UseExternalService": false,
    "ContainerName": "TC-CloudGames-Postgres",
    "Host": "localhost",
    "Port": 5432,
    "UsersDbName": "tc-cloudgames-users-db",
    "GamesDbName": "tc-cloudgames-games-db",
    "PaymentsDbName": "tc-cloudgames-payments-db",
    "MaintenanceDbName": "postgres",
    "User": "postgres",
    "Password": "postgres",
    "Schema": "public",
    "ConnectionTimeout": 30
  },
  "RabbitMq": {
    "UseExternalService": false,
    "ContainerName": "TC-CloudGames-RabbitMq",
    "Host": "localhost",
    "Port": 5672,
    "VirtualHost": "/",
    "UserName": "guest",
    "Password": "guest",
    "Exchange": "user.events",
    "AutoProvision": true,
    "Durable": true,
    "UseQuorumQueues": true,
    "AutoPurgeOnStartup": true
  },
  "AzureServiceBus": {
    "UseExternalService": false,
    "ContainerName": "TC-CloudGames-AzureServiceBus",
    "ConnectionString": "",
    "TopicName": "user-events",
    "SubscriptionName": "fanout-subscription",
    "AutoProvision": true,
    "MaxDeliveryCount": 10,
    "EnableDeadLettering": true,
    "AutoPurgeOnStartup": true,
    "UseControlQueues": true
  },
  "Cache": {
    "UseExternalService": false,
    "ContainerName": "TC-CloudGames-Redis",
    "Host": "localhost",
    "Port": 6379,
    "Password": "Redis@123",
    "InstanceName": "TC.CloudGames.Users.Api:",
    "Secure": false
  }
}

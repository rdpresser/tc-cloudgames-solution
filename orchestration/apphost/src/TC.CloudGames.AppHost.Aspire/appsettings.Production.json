{
  "MessageBroker": {
    "Type": "AzureServiceBus"
  },
  "Database": {
    "UseExternalService": true,
    "Host": "<prod-db-host>",
    "Port": 5432,
    "UsersDbName": "tc-cloudgames-users-db",
    "GamesDbName": "tc-cloudgames-games-db",
    "PaymentsDbName": "tc-cloudgames-payments-db",
    "MaintenanceDbName": "postgres",
    "User": "<prod-db-user>",
    "Password": "<prod-db-password>",
    "Schema": "public",
    "ConnectionTimeout": 30
  },
  "AzureServiceBus": {
    "UseExternalService": true,
    "ContainerName": "TC-CloudGames-AzureServiceBus",
    "ConnectionString": "Endpoint=sb://<namespace>.servicebus.windows.net/;SharedAccessKeyName=<key-name>;SharedAccessKey=<key-value>",
    "Namespace": "<namespace>.servicebus.windows.net",
    "UsersTopicName": "user.events",
    "GamesTopicName": "game.events",
    "PaymentsTopicName": "payment.events",
    "AutoProvision": true,
    "MaxDeliveryCount": 10,
    "EnableDeadLettering": true,
    "AutoPurgeOnStartup": false,
    "UseControlQueues": true
  },
  "Cache": {
    "UseExternalService": true,
    "Host": "<prod-cache-host>",
    "Port": 6380,
    "Password": "<prod-cache-password>",
    "InstanceNames": {
      "users": "TC.CloudGames.Users.Api:",
      "games": "TC.CloudGames.Games.Api:",
      "payments": "TC.CloudGames.Payments.Api:"
    },
    "Secure": true
  },
  "GrafanaCloud": {
    "UseExternalService": true,
    "ContainerName": "TC-CloudGames-GrafanaCloud",
    "LogsApiToken": "<placeholder for GRAFANA_LOGS_API_TOKEN>",
    "OtelPrometheusApiToken": "<placeholder for GRAFANA_OTEL_PROMETHEUS_API_TOKEN>",
    "OtelGamesResourceAttributes": "service.name=tccloudgames-games,service.namespace=tccloudgames,deployment.environment=production",
    "OtelUsersResourceAttributes": "service.name=tccloudgames-users,service.namespace=tccloudgames,deployment.environment=production",
    "OtelPaymentsResourceAttributes": "service.name=tccloudgames-payments,service.namespace=tccloudgames,deployment.environment=production",
    "OtelExporterOtlpEndpoint": "https://otlp-gateway-prod-sa-east-1.grafana.net/otlp",
    "OtelExporterOtlpProtocol": "http/protobuf",
    "OtelExporterOtlpHeaders": "<placeholder for OTEL_EXPORTER_OTLP_HEADERS>"
  }
}
